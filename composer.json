{
    "name": "goaop/goaop-laminas-module",
    "description": "Integration module for Go! AOP Framework",
    "type": "library",
    "keywords": [
        "module",
        "laminas",
        "aop",
        "php",
        "aspect"
    ],
    "license": "MIT",
    "require": {
        "goaop/framework": "^1.0|^2.0|^3.0",
        "laminas/laminas-modulemanager": "^2.0",
        "laminas/laminas-dependency-plugin": "^1.0 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0 | ^6.0 | ^9.0",
        "laminas/laminas-mvc": "^2.0 | ^3.0",
        "laminas/laminas-view": "^2.0",
        "laminas/laminas-serializer": "^2.0",
        "laminas/laminas-log": "^2.0",
        "laminas/laminas-i18n": "^2.0",
        "laminas/laminas-console": "^2.0",
        "symfony/console": "^3.0 | ^4.0"
    },
    "suggest": {
        "symfony/console": "Required to use goaop console warmup"
    },
    "authors": [
        {
            "name": "Lisachenko Alexander",
            "email": "lisachenko.it@gmail.com"
        },
        {
            "name": "Andreas Fr√∂mer",
            "email": "blubb0r05+github@gmail.com"
        },
        {
            "name": "Danny Meyer",
            "email": "danny.meyer@ravenc.de"
        }
    ],
    "bin": [
        "bin/goaop-laminas-warmup"
    ],
    "autoload": {
        "psr-4": {
            "Go\\Laminas\\Framework\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Go\\Laminas\\Framework\\Tests\\": "tests/"
        }
    }
}
